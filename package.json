        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/eslint-config-typescript": "^14.5.1",
        "autoprefixer": "^10.4.12",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "eslint": "^9.29.0",
        "eslint-plugin-prettier": "^5.5.0",
        "eslint-plugin-vue": "^10.2.0",
        "laravel-vite-plugin": "^1.2.0",
        "postcss": "^8.4.31",
        "prettier": "^3.5.3",
